{
  "name": "n0de-platform",
  "version": "1.0.0",
  "description": "Comprehensive platform for Solana RPC service management, analytics, and profitability assessment",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node start-production.js",
    "start:production": "node start-production.js",
    "dev": "nodemon src/server.js",
    "build": "npm run migrate && npm run seed",
    "migrate": "node src/database/migrate.js",
    "seed": "node src/database/seed.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "format:check": "prettier --check src/**/*.js",
    "test-health": "node src/health/rpc-health-tester.js",
    "test-latency": "node src/latency/latency-analyzer.js",
    "test-mev": "node src/mev/enhanced-mev-detector.js",
    "analyze-revenue": "node src/revenue/revenue-calculator.js",
    "competitive-analysis": "node src/competitive/competitor-benchmark.js",
    "stress-test": "node src/stress/load-tester.js",
    "full-assessment": "node src/index.js",
    "generate-report": "node scripts/generate-assessment-report.js",
    "serve-dashboard": "node scripts/serve-dashboard.js",
    "benchmark-competitors": "node scripts/benchmark-all-competitors.js",
    "optimize-config": "node scripts/optimize-configuration.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "chalk": "^5.6.0",
    "cors": "^2.8.5",
    "express": "^4.19.0",
    "ioredis": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "playwright": "^1.55.0",
    "uuid": "^10.0.0"
  },
  "keywords": [
    "solana",
    "rpc",
    "blockchain",
    "mev",
    "arbitrage",
    "performance",
    "profitability",
    "assessment",
    "benchmarking",
    "revenue-analysis"
  ],
  "author": "n0de",
  "license": "MIT"
}
