{
  "name": "n0de-backend",
  "version": "1.0.0",
  "description": "n0de RPC Infrastructure Backend API",
  "author": "n0de Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "npx prisma generate",
    "build": "nest build",
    "format": "prettier --write \"backend/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run start:prod",
    "dev": "node scripts/dev.js",
    "dev:backend": "ts-node -r tsconfig-paths/register backend/main.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:pm2": "pm2 start ecosystem.dev.config.js",
    "dev:watch": "node scripts/config-watcher.js",
    "start:dev": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node dist/backend/main.js",
    "start:cluster": "pm2 start ecosystem.prod.config.js",
    "lint": "eslint \"{backend,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^5.2.0",
    "@nestjs/websockets": "^10.4.20",
    "@prisma/client": "^6.15.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/ioredis": "^4.28.10",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/winston": "^2.4.4",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "colors": "^1.4.0",
    "compression": "^1.8.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv-vault": "^1.27.0",
    "express-rate-limit": "^7.5.1",
    "geoip-lite": "^1.4.10",
    "helmet": "^7.2.0",
    "ioredis": "^5.7.0",
    "moment": "^2.30.1",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.10",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "playwright": "^1.55.0",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2",
    "socket.io": "^4.7.4",
    "stripe": "^18.5.0",
    "typescript": "^5.9.2",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.20",
    "@stripe/mcp": "^0.2.4",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.12",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chokidar": "^4.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "lighthouse": "^12.8.2",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "ws": "^8.18.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "backend",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
